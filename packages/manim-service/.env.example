# Manim Service Configuration
# Copy this file to .env and modify as needed

# Port for the Flask service
PORT=5001

# Flask environment (development/production)
FLASK_ENV=development

# Directory where generated videos will be saved
MANIM_OUTPUT_DIR=./output

# Logging level
LOG_LEVEL=INFO

# Optional: Manim quality settings
# MANIM_QUALITY=medium_quality

# Optional: Enable/disable caching
# MANIM_DISABLE_CACHING=true